          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="CPR Last Digit for Male is Odd">
            <stringProp name="BeanShellAssertion.query">String gender = vars.get(&quot;gender&quot;);

String lastDigitStr = vars.get(&quot;cpr_last_digit&quot;);
if (lastDigitStr != null &amp;&amp; !lastDigitStr.isEmpty()) {     
	int lastDigit = Integer.parseInt(lastDigitStr);

	if (&quot;male&quot;.equalsIgnoreCase(gender)) {
    if (lastDigit % 2 != 1) {
        Failure = true;
        FailureMessage = &quot;Male CPR &quot; + cpr + &quot; has an even last digit&quot;;
    }
}
	
}

</stringProp>
            <stringProp name="BeanShellAssertion.filename"></stringProp>
            <stringProp name="BeanShellAssertion.parameters"></stringProp>
            <boolProp name="BeanShellAssertion.resetInterpreter">false</boolProp>
          </BeanShellAssertion>
          <hashTree/>
          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="CPR Last Digit for Female is Even" enabled="false">
            <stringProp name="BeanShellAssertion.query">int lastDigit = Integer.parseInt(vars.get(&quot;cpr_last_digit&quot;));
String gender = vars.get(&quot;gender&quot;);

if (&quot;female&quot;.equalsIgnoreCase(gender)) {
    if (lastDigit % 2 != 0) {
        Failure = true;
        FailureMessage = &quot;Female CPR &quot; + cpr + &quot; has an odd last digit&quot;;
    }
}</stringProp>
            <stringProp name="BeanShellAssertion.filename"></stringProp>
